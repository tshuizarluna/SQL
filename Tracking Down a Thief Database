CREATE TABLE alibi_info (
  `alibi_id` INTEGER,
  `suspect_id` INTEGER,
  `alibi_location` VARCHAR(14),
  `date_time` VARCHAR(14),
  `alibi_statement` VARCHAR(75)
);

