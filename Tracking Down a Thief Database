CREATE TABLE suspect_info (
  `suspect_id` INTEGER,
  `suspect_first_name` VARCHAR(9),
  `suspect_last_name` VARCHAR(12),
  `height_cm` INTEGER,
  `age` INTEGER,
  `eye_color` VARCHAR(5)
);

